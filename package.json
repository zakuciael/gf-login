{
  "name": "@zakku/gf-login",
  "version": "4.0.0",
  "description": "A node.js library for authenticating to GameFail's servers.",
  "homepage": "https://gf-login.zakku.eu",
  "sideEffect": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/zakuciael/gf-login.git",
  "author": "Krzysztof Saczuk <zakuciael@outlook.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=16.11.7"
  },
  "keywords": [
    "nostale",
    "login",
    "gameforge",
    "captcha"
  ],
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "yarn build:source && yarn build:declarations",
    "build:source": "rimraf dist/ && tsc -p tsconfig.json",
    "build:declarations": "rimraf declarations/ && tsc -p tsconfig.json --declaration --declarationMap --declarationDir declarations/ && api-extractor run",
    "build:docs": "typedoc",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "yarn lint && yarn build"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.18.11",
    "@types/dateformat": "^5.0.0",
    "@types/node": "^16.11.7",
    "@types/node-fetch": "^2.5.8",
    "@types/node-forge": "^1.3.0",
    "@types/uuid": "^8.3.0",
    "@types/which": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.15",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@zakku/ez-captcha": "^1.0.2",
    "cfv": "^1.1.0",
    "dateformat": "^5.0.3",
    "node-fetch": "^3.2.10",
    "node-forge": "^1.3.1",
    "untildify": "^4.0.0",
    "uuid": "^9.0.0",
    "which": "^2.0.2"
  }
}
